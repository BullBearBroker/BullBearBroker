# QA: configuración de cobertura enfocada en backend con exclusiones mínimas
[run]
branch = True
source = backend
omit =
    backend/alembic/*
    backend/alembic/versions/*
    backend/scripts/*
    backend/tests/*

[report]
show_missing = True
skip_covered = True
precision = 1
exclude_lines =
    pragma: no cover
    if TYPE_CHECKING:
    if __name__ == .__main__.:
